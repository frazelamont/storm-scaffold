  "browserlist": [
    "ie >= 9",
    "ie_mob >= 10",
    "ff >= 20",
    "chrome >= 4",
    "safari >= 7",
    "opera >= 23",
    "ios >= 7",
    "android >= 4.4",
    "bb >= 10"
  ],
  "jest": {
    "verbose": true,
    "preset": "jest-puppeteer"
  },
  "devDependencies": {
