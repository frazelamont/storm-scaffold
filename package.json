{
  "name": "storm-scaffold",
  "version": "1.4.1",
  "description": "Project scaffolder",
  "repository": "https://github.com/storm/storm-scaffold",
  "author": "stormid",
  "license": "ISC",
  "scripts": {
    "start": "node_modules/.bin/gulp serve",
    "compile": "node_modules/.bin/gulp compile --production",
    "watch": "node_modules/.bin/gulp watch",
    "test": "cross-env NODE_ENV=test node node_modules/jest/bin/jest.js --forceExit",
    "disc": "discify build/static/js/app.js >disc.html"
