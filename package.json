    "axe-core": "3.1.2",
    "babel-preset-env": "1.7.0",
    "babelify": "10.0.0",
    "body-parser": "1.18.3",
    "browser-sync": "2.23.6",
    "browserify": "16.2.3",
    "cross-env": "5.2.0",
